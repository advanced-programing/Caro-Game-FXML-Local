<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/lableStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofxml.ChooseLevelBoxController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" styleClass="background">
         <top>
            <Label prefHeight="155.0" prefWidth="341.0" text="Choose Level of Game" BorderPane.alignment="CENTER">
               <font>
                  <Font size="33.0" />
               </font>
            </Label>
         </top>
         <center>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="easyLevel" mnemonicParsing="false" onAction="#easyLevelAction" prefHeight="30.0" prefWidth="90.0" styleClass="button2" text="Easy">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="mediumLevel" mnemonicParsing="false" onAction="#mediumLevelAction" prefHeight="30.0" prefWidth="90.0" styleClass="button2" text="Medium">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="hardLevel" mnemonicParsing="false" onAction="#hardLevelAction" prefHeight="30.0" prefWidth="90.0" styleClass="button2" text="Hard">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="homeButton" mnemonicParsing="false" onAction="#goHome" prefHeight="30.0" prefWidth="90.0" styleClass="button3" text="Home">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
