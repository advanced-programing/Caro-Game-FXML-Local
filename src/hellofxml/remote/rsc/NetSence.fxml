<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="500" prefWidth="390" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofxml.remote.java.NetSenceController">
    <stylesheets>
        <URL value="@netsence.css" />
    </stylesheets>
    <children>
        <GridPane maxWidth="390.0" prefHeight="500.0">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" prefWidth="195.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" prefWidth="195.0" />
            </columnConstraints>
            <rowConstraints>
            <RowConstraints prefHeight="390.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="422.0" minHeight="10.0" prefHeight="144.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="148.0" minHeight="10.0" percentHeight="10.0" prefHeight="63.0" valignment="CENTER" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <HBox fx:id="pane_board" GridPane.columnSpan="2" />
                <Label id="lb_turn" fx:id="lb_turn" alignment="TOP_LEFT" text="Networked Caro Game" textFill="#1fb230" GridPane.columnSpan="2" GridPane.rowIndex="1" />
                <Label id="lb_message" fx:id="lb_message" alignment="CENTER" contentDisplay="CENTER" text="message" GridPane.columnSpan="2" GridPane.rowIndex="2" />
            </children>
        </GridPane>
    </children>
</AnchorPane>
